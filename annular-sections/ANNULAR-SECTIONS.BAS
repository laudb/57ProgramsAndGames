REM THIS PROGRAM COMPUTES THE VARIOUS 
REM PARAMETERS CONNECTED WITH AN ANNULAR
REM SECTION
PRINT "INSIDE DIAMETER(D1) = ";
INPUT D1
PRINT "OUTSIDE DIAMETER (D2) = ";
INPUT D2
LET P = 3.14159
LET I = (P*((D2 &uarr; 4) - (D1 &uarr; 4)))/64
LET J = I*2
LET A = (P*((D2 &uarr; 2) - (D1 &uarr; 2)))/4
PRINT "MOMENT OF INTERTIA = ";I
PRINT "POLAR MOMENT OF INERTIA = ";J
PRINT "AREA OF SECTION =";A
PRINT
PRINT "TYPE 1 TO CONTINUE, 0 TO STOP"
INPUT L
IF L = 1 THEN 190
STOP
PRINT 
GO TO 40
END
